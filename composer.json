{
    "name": "artembabak/laravel-migration-builder-salesforce",
    "description": "Create migration of object structure from Salesforce Org to a local database.",
    "type": "library",
    "require": {
        "php": "^8.2",
        "omniphx/forrest": "^2.2",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "artembabak/laravel-migration-builder": "dev-master"
    },
    "authors": [
        {
            "name": "David Hayes",
            "email": "david@blackbrick.software"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/artembabak/laravel-migration-builder"
        }
    ],
    "autoload": {
        "psr-4": {
            "BlackBrickSoftware\\MigrationBuilderSalesforce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackBrickSoftware\\MigrationBuilderSalesforce\\MigrationBuilderSalesforceServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
